	doi = {10.1007/s11214-014-0073-3},
	issn = {1572-9672},
	journal = {Space Science Reviews},
	month = {Apr},
	number = {1},
	pages = {357--392},
	title = {Pre-flight and On-orbit Geometric Calibration of the Lunar Reconnaissance Orbiter Camera},
